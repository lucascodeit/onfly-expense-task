{
  "name": "onfly",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "npm run build && node dist/main.js",
    "dev": "nodemon --watch src/ --exec 'ts-node src/main.ts' -e ts",
    "migration:create": "npx typeorm-ts-node-esm migration:create src/common/db/migrations/$MIGRATION_NAME",
    "migration:generate": "npx typeorm-ts-node-esm -d src/common/db/data-source-migration.ts migration:generate src/common/db/migrations/$MIGRATION_NAME",
    "migration:run": "npx typeorm-ts-node-esm -d src/common/db/data-source-migration.ts migration:run",
    "migration:revert": "npx typeorm-ts-node-esm -d src/common/db/data-source-migration.ts migration:revert"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "mysql2": "^3.9.7",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^20.12.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "description": "Make with love for onfly job opportunity",
  "main": "index.js",
  "author": "Lucas Correa",
  "license": "MIT",
  "private": false
}
